#!/bin/env ruby
#encoding: utf-8

require 'open-uri'
require 'nokogiri'
require 'json'
require 'mechanize'
require 'execjs'

a = Mechanize.new
page = a.get('https://maps.google.com/maps?q=hospitals+near+Dalyan Mh.,+4148.+Sokak,+35930 Çeşme/Izmir+Province,+Turkey')


gsForm = page.forms[0]
gsForm.encoding = 'utf-8'
gForm = gsForm.submit
gDoc = gForm.parser
data = gDoc.css('script').text.to_s[64..-4][0..-14]


puts data



